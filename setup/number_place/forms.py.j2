# coding: utf-8

from django.forms import ChoiceField, Form
CHOICES = (
	('', ''),{% for i in range(1, 9 + 1) %}
    ({{ i }}, '{{ i }}'),{% endfor %}
)
{% for key in forms %}{% set f = forms[key] %}
class Form{{ key }}(Form):{% for n in f.names %}
    {{ n }} = ChoiceField(choices=CHOICES, initial='', required=False){% endfor %}
{% endfor %}
